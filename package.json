{
  "name": "Megapytsker",
  "version": "0.1.0",
  "description": "Megapytsker project repository",
  "main": "index.js",
  "repository": "https://vikmarchenko@bitbucket.org/vikmarchenko/megapytsker.git",
  "author": "Viktor Marchenko <viktor.marchenko@gmail.com>",
  "private": true,
  "scripts": {
    "start": "cross-env NODE_ENV=development babel-watch -x web server/index.js",
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules",
    "seed": "cd server && sequelize db:migrate && babel-node seeders/read-ticket.js",
    "watch": "ava --watch",
    "test": "nyc ava && nyc report --reporter=lcov"
  },
  "ava": {
    "files": [
      "./server/actions/inputText.test.js"
    ],
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  },
  "dependencies": {
    "adbkit": "^2.7.0",
    "express": "^4.14.1",
    "foundation-sites": "^6.3.1",
    "sequelize": "^3.30.2",
    "sequelize-cli": "^2.5.1",
    "serialport": "^4.0.7",
    "socket.io": "^1.7.2",
    "sqlite3": "^3.1.8",
    "then-sleep": "^1.0.1",
    "vue": "^2.1.0",
    "vuedraggable": "^2.9.0",
    "vuex": "^2.1.2"
  },
  "devDependencies": {
    "ava": "^0.18.2",
    "nyc": "^10.1.2",
    "babel-cli": "^6.22.2",
    "babel-core": "^6.0.0",
    "babel-loader": "^6.0.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-watch": "^2.0.6",
    "cross-env": "^3.0.0",
    "css-loader": "^0.25.0",
    "file-loader": "^0.9.0",
    "node-sass": "^4.5.0",
    "sass-loader": "^5.0.1",
    "svg-sprite-loader": "^0.3.0",
    "vue-loader": "^10.0.0",
    "vue-template-compiler": "^2.1.0",
    "webpack": "^2.2.0",
    "webpack-dev-middleware": "^1.10.0",
    "webpack-hot-middleware": "^2.16.1"
  }
}
